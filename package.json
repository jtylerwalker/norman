{
  "name": "norman",
  "version": "0.0.1",
  "description": "Library that normalizes json for consumption",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll --notify"
  },
  "author": "Tyler Walker",
  "license": "ISC",
  "dependencies": {
    "jest": "^23.6.0"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.?(m)js?(x)",
      "**/?(*.)(spec|test).?(m)js?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "es2015",
            {
              "modules": false
            }
          ]
        ],
        "plugins": [
          [
            "transform-es2015-modules-commonjs",
            {
              "spec": true
            }
          ]
        ]
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-es2015": "^6.24.1"
  }
}
